  "dependencies": {
    "ant-design-vue": "^1.3.13",
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.1",
    "vuex-class": "^0.3.2",
    "vuex-router-sync": "^5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": 0,
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "none",
          "singleQuote": true
        }
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 100,
    "trailingComma": "none",
    "singleQuote": true,
    "jsxBracketSameLine": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "libVersion": "1.0.0",
  "library": {
    "vuelib": [
      "vue/dist/vue.esm.js",
      "vue-router",
      "vuex-router-sync",
      "vuex"
    ],
    "ant": [
      "ant-design-vue"
    ],
    "tools": [
      "axios",
      "lodash",
      "moment",
      "nprogress"
    ]
  }
